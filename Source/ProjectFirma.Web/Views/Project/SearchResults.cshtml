@using System.Linq
@using ProjectFirma.Web.Views.Project
@inherits SearchResults

<h3 style="margin-bottom: 20px">Results for project search keywords "@ViewDataTyped.SearchCriteria".</h3>

@if (!ViewDataTyped.EntitySearchResults.Any())
{
    <p>There are no projects matching your search keywords.</p>
}
else
{
    <p>@ViewDataTyped.EntitySearchResults.Count projects found.</p>

    foreach (var findResult in ViewDataTyped.EntitySearchResults)
    {
        <div>
            @findResult.DisplayNameAsUrl
        </div>
    }
}